program test_builtins;
	var x : array [10] of char;
	begin
		writeInteger(3);
		writeChar('\n');
		writeChar('\t');
		writeChar('\n');
		writeBoolean(true);
		writeChar('\n');
		writeBoolean(false);
		writeChar('\n');
		writeReal(3.000);
		writeChar('\n');
		writeReal(3);
		writeChar('\n');
		writeString("Hello World!\n");
		writeInteger(abs(-3));
		writeChar(' ');
		writeInteger(abs(3));
		writeChar('\n');
		writeReal(fabs(-3.125));
		writeChar(' ');
		writeReal(fabs(3.985));
		writeChar('\n');
		writeReal(sqrt(3.985));
		writeChar('\n');
		writeReal(sin(pi()));
		writeChar('\n');
		writeReal(cos(pi()));
		writeChar('\n');
		writeReal(tan(pi()));
		writeChar('\n');
		writeReal(arctan(pi()));
		writeChar('\n');
		writeReal(exp(ln(2)));
		writeChar('\n');
		writeReal(ln(exp(1)));
		writeChar('\n');
		writeInteger(trunc(3.6));
		writeChar('\n');
		writeInteger(round(3.6));
		writeChar('\n');
		writeInteger(ord('A'));
		writeChar('\n');
		writeChar(chr(65));
		writeChar('\n');
		writeInteger(readInteger());
		writeChar('\n');
		writeBoolean(readBoolean());
		writeChar('\n');
		writeChar(readChar());
		writeChar('\n');
		writeReal(readReal());
		writeChar('\n');
		readString(5, x);
		writeString(x);
	end.
